-- Name: catalog_seocategory_id_seq; Type: SEQUENCE SET; Schema: public; Owner: rupechi-ru
--

SELECT pg_catalog.setval('public.catalog_seocategory_id_seq', 2915, true);


--
-- Name: catalog_seocategory catalog_seocategory_domain_id_category_id_65a349b3_uniq; Type: CONSTRAINT; Schema: public; Owner: rupechi-ru
--

ALTER TABLE ONLY public.catalog_seocategory
    ADD CONSTRAINT catalog_seocategory_domain_id_category_id_65a349b3_uniq UNIQUE (domain_id, category_id);


--
-- Name: catalog_seocategory catalog_seocategory_pkey; Type: CONSTRAINT; Schema: public; Owner: rupechi-ru
--

ALTER TABLE ONLY public.catalog_seocategory
    ADD CONSTRAINT catalog_seocategory_pkey PRIMARY KEY (id);


--
-- Name: catalog_seocategory_category_id_452b870c; Type: INDEX; Schema: public; Owner: rupechi-ru
--

CREATE INDEX catalog_seocategory_category_id_452b870c ON public.catalog_seocategory USING btree (category_id);


--
-- Name: catalog_seocategory_domain_id_f06f5232; Type: INDEX; Schema: public; Owner: rupechi-ru
--

